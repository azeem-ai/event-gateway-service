# .env.example

# URL of your AWS SQS queue
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/event-ingest-queue

# Target GraphQL API where enriched events are sent
GRAPHQL_ENDPOINT=https://your-api/graphql

# Bearer token used to authorize the GraphQL request
AUTH_TOKEN=secure-token-here

# Brand name to enrich incoming events before dispatching
BRAND_NAME=brandName